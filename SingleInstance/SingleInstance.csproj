<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netcoreapp3.1</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <Authors>HMBSbige</Authors>
    <Description>Enforces single instance for an application.</Description>
    <RepositoryUrl>https://github.com/HMBSbige/SingleInstance</RepositoryUrl>
    <PackageProjectUrl>https://github.com/HMBSbige/SingleInstance</PackageProjectUrl>
    <PackageTags>SingleInstance</PackageTags>
    <Version>1.0.1</Version>
    <Copyright>Copyright © HMBSbige 2020</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>HMBSbige.SingleInstance</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Reactive" Version="4.4.1" />
  </ItemGroup>

</Project>
